{
  "id": "search",
  "image": "pedrokuhnmaster/elasticsearch:6.1.4-3.0.3-2",
  "loadBalancer": {
    "targetPort": 80
  },
  "readinessProbe": {
    "httpGet": {
      "path": "/",
      "port": 80
    }
  },
  "livenessProbe": {
    "httpGet": {
      "path": "/",
      "port": 80
    }
  },
  "memory": 512,
  "cpu": 1
}
